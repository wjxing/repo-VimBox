<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="3rd_plug"
        fetch="https://github.com" />

    <remote name="self"
        fetch="ssh://git@github.com" />

    <default revision="master"
        remote="3rd_plug"
        sync-j="4"
        sync-c="true" />

    <include name="common.xml" />

    <project path="bundle/a" name="vim-scripts/a.vim"/>
    <project path="bundle/c" name="vim-scripts/c.vim"/>
    <project path="bundle/ctrlp" name="ctrlpvim/ctrlp.vim.git"/>
    <project path="bundle/genutils" name="vim-scripts/genutils"/>
    <project path="bundle/gtags" name="vim-scripts/gtags.vim.git"/>
    <project path="bundle/mark" name="vim-scripts/Mark.git"/>
    <project path="bundle/taglist" name="vim-scripts/taglist.vim"/>
    <project path="bundle/unite" name="Shougo/unite.vim.git"/>
    <project path="bundle/gitgutter" name="vim-scripts/vim-gitgutter.git"/>
</manifest>
